<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= `CSSless ${title} ` %></h1>
    <p>Welcome to <%= title %></p>
    <ul>
      <% messages.forEach(option => { %>
        <li><%= `${option.user}: ${option.text} ` %><br><%= ` ${option.added} ` %></li>
        <br>
      <% }); %>
    </ul>
    <a href="./new">
      <button>New message</button>
    </a>
  </body>
</html>
